{
    "manifest_version": 3,
    "name": "My Web Extension",
    "version": "0.6.0",
    "description": "A sample Chrome extension.",
    "permissions": ["activeTab"],
    "action": {
      "default_icon": {
        "16": "images/icon-autocv-cr-16.png",
        "48": "images/icon-autocv-cr-48.png",
        "128": "images/icon-autocv-cr-128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["bundle.js"]
      }
    ]
}
  